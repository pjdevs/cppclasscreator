{
	"name": "cppclasscreator",
	"displayName": "C++ Class Creator",
	"description": "A C++ class creation extension for VSCode",
	"version": "0.0.1",
	"author": {
		"name": "pjdevs",
		"email": "pj33720@gmail.com"
	},
	"repository": "https://github.com/pjdevs/cppclasscreator",
	"engines": {
		"vscode": "^1.50.0"
	},
	"license": "GPL-3.0",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cppclasscreator.createClass"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cppclasscreator.createClass",
				"title": "C++: Create Class"
			}
		],
		"keybindings":[
			{
				"command": "cppclasscreator.createClass",
				"key": "ctrl+alt+c",
				"mac": "cmd+alt+c",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.50.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
